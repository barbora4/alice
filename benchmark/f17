(and (zeroin X)
(neg
(exists Z
(exists Y
(and (and (sing Z) (sing Y))
(or
 (and
  (and
   (and (and (succ Y Z) (sing Y)) (and (sing Y) (sing Z)))
   (and (sing Y) (and (sing Z) (sub Z X)))
  )
  (and (sing Z) (and (sing Y) (and (sub Y X) (sing Y))))
 )
 (and
  (and
   (and (succ Y Z) (sing Y))
   (and (sing Z) (and (sing Y) (neg (and (sing Z) (sub Z X)))))
  )
  (and (sing Z) (and (sing Y) (neg (and (sing Y) (sub Y X)))))
 )
)
)))))
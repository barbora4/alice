#define (fair A B) (and (neg (exists U (and (sing U) (neg (and (sing U) (implies (sub U A) (exists V (and (< U V) (and (sing V) (sub V B)))))))))) (neg (exists U (and (sing U) (neg (and (sing U) (implies (sub U B) (exists V (and (< U V) (and (sing V) (sub V A)))))))))))

(forall X
(implies
  (fair X Y)
  (fair Y Z)
)
)
